{
    "multipart": [
        {   "apply": { "model": "fenceoverhaul:acacia_fence_core" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north", "y": 270, "uvlock": true }
        },
        {   "when": { "north_east": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north_east"}
        },
        {   "when": { "south_east": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north_east", "y":90 }
        },
        {   "when": { "south_west": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north_east", "y":180 }
        },
        {   "when": { "north_west": "true" },
            "apply": { "model": "fenceoverhaul:acacia_fence_sub_north_east", "y":270}
        }
    ]
}